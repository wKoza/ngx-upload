{
  "name": "ngx-upload-workspace",
  "version": "16.0.1",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx run-many --all --target=test",
    "release:git": "git add package.json && git add libs/ngx-upload/package.json && git commit -m \"chore: bump version number\" && standard-version --first-release && git push --follow-tags origin master",
    "release:npm": "npm run build ngx-upload && npm publish --access=public dist/libs/ngx-upload",
    "gh-pages": "git checkout gh-pages && git merge master --no-edit --no-ff && del-cli *.js *.js.map && npm run compodoc && git add . && git commit -m \"chore: build docs\" && git push && git checkout master",
    "compodoc": "compodoc -p tsconfig-compodoc.json -d docs --disableCoverage --disablePrivateOrInternalSupport"
  },
  "private": true,
